\")]),s(\"td\",[s(\"img\",{attrs:{src:t(\"7cba\"),alt:\"GM1\"}})]),s(\"td\",[s(\"img\",{attrs:{src:t(\"ce50\"),alt:\"GM2\"}})]),s(\"td\",[s(\"img\",{attrs:{src:t(\"ce70\"),alt:\"GM3\"}})]),s(\"td\",[s(\"img\",{attrs:{src:t(\"b39f\"),alt:\"GM4\"}})]),s(\"td\",[s(\"img\",{attrs:{src:t(\"8e01\"),alt:\"GM5\"}})])]),s(\"tr\",[s(\"td\",[a._v(\"Color de referencia\")]),s(\"td\",[s(\"img\",{attrs:{src:t(\"f212\"),alt:\"GM1\"}})]),s(\"td\",[s(\"img\",{attrs:{src:t(\"31c7\"),alt:\"GM2\"}})]),s(\"td\",[s(\"img\",{attrs:{src:t(\"edc4\"),alt:\"GM3\"}})]),s(\"td\",[s(\"img\",{attrs:{src:t(\"22da\"),alt:\"GM4\"}})]),s(\"td\",[s(\"img\",{attrs:{src:t(\"1ab0\"),alt:\"GM5\"}})])])])])])])])},function(){var a=this,s=a._self._c;return s(\"div\",{staticClass:\"titulo-segundo\",attrs:{id:\"t_2_2\"}},[s(\"div\",{staticClass:\"h2\",attrs:{\"data-aos\":\"fade-right\"}},[a._v(\"2.2 Poscosecha\")])])},function(){var a=this,s=a._self._c;return s(\"div\",{staticClass:\"row mb-5\"},[s(\"div\",{staticClass:\"col-sm-6 col-lg-3 mb-4 mb-lg-0\"},[s(\"div\",{staticClass:\"tarjeta--boton p-5\"},[s(\"div\",{staticClass:\"row justify-content-center mb-3\"},[s(\"div\",{staticClass:\"col-7\"},[s(\"figure\",[s(\"img\",{attrs:{src:t(\"f3bc\"),alt:\"Texto que describa la imagen\"}})])])]),s(\"h3\",{staticClass:\"text-center\"},[a._v(\"Lavado\")]),s(\"p\",{staticClass:\"text-small\"},[a._v(\"Eliminación de residuos e impurezas.\")])])]),s(\"div\",{staticClass:\"col-sm-6 col-lg-3 mb-4 mb-lg-0\"},[s(\"div\",{staticClass:\"tarjeta--boton p-4\"},[s(\"div\",{staticClass:\"row justify-content-center mb-3\"},[s(\"div\",{staticClass:\"col-7\"},[s(\"figure\",[s(\"img\",{attrs:{src:t(\"e806\"),alt:\"Texto que describa la imagen\"}})])])]),s(\"h3\",{staticClass:\"text-center\"},[a._v(\"Clasificación\")]),s(\"p\",{staticClass:\"text-small\"},[a._v(\"Selección de los frutos según estándares de mercado.\")])])]),s(\"div\",{staticClass:\"col-sm-6 col-lg-3 mb-4 mb-sm-0\"},[s(\"div\",{staticClass:\"tarjeta--boton p-4\"},[s(\"d